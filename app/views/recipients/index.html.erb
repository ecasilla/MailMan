
<h3>Search Contact List</h3> 

 <%= form_for(@recipents) do |f| %>
    
    <%= f.file_field :recipents %>
    <%= f.label :Contacts,  %><br/>
    <%= f.text_field :recipents %>

<% end %>
  

<%= form_tag("/recipents", method: "get") do %>
  <%= label_tag(:q, "Search for Contacts:") %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag("Search") %>
<% end %>

