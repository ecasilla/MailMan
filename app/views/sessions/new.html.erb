<div class="divider"></div>
<center>
<%= link_to "Sign Up", new_user_path, class: "SignIn btn btn-primary" %>

<%= form_tag session_path, :method => :post , class: 'form-horizontal' do %>

  <%= label_tag 'email', 'Email' %>
  <%= email_field_tag :email %>

  <%= label_tag 'password', 'Password' %>
  <%= password_field_tag :password %>

<%= submit_tag 'Sign In', class: "btn btn-primary SignIn" %>
<% end %>
</center>